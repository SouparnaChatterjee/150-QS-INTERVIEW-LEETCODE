class Solution {
public:
    int removeElement(vector<int>& nums, int val) {
        int i=0;
        for(auto it:nums)
        {
            if(it!=val)
            //this is num[i] and then i++
            //nums[i++]=it;
            {
                nums[i]=it;
                i++;
            }
        }
        return i;
    }
};
